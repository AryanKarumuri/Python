str = "Hello" 
str1 = "Tiger"

print(type(str))
#Output: <class 'str'>

print(str)
#Output: Hello

print("The string str : %s"%(str))
#Output: The string str : Hello

print(f"The String str is {str}")
#Output: The String str is Hello

print("The String str is {}" .format(str))
#Output: The String str is Hello

print("The Strings are {0} and {1}" .format(str, "Lion"))
#Output: The String are Hello and Lion

print("The Strings str and str1 are "+str+" and "+str1)
#Output: The String str and str1 are Hello and Tiger

print(f"The Strings str and str1 are {str} and {str1}")
#Output: The String str and str1 are Hello and Tiger
