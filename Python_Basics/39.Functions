import random
#Function declaration
def display(name, quote):
    print(f"Hello {name}, welcome to my Github repository..")
    print(quote)

name = input("Enter your name: ")
quotes = ['Have a nice day!!', 'Enjoy your learning path^_^', 'Happy learning']
quote = random.choice(quotes)

#Function call
display(name, quote)

